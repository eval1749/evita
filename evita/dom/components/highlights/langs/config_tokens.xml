<tokens id="config">
  <token name="comment" type="comment" pattern="#[^\n]*" />
  <token name="string1" type="string_literal" pattern="'([^\\']|\\.)*'" />
  <token name="string2" type="string_literal" pattern='"([^\\"]|\\.)*"' />
  <token name="name" type="identifier" pattern="\w+" />
  <token name="punctuation" type="operator" pattern="[^\w\s]+" />
  <token name="whitespace" type="whitespace" pattern="\s+" />
</tokens>
