interface EventTarget {
  void addEventListener(DOMString type, EventListener callback,
                        boolean capture);
  void addEventListener(DOMString type, EventListener callback);

  [RaisesException]
  boolean dispatchEvent(Event event);

  void removeEventListener(DOMString type, EventListener callback,
                           boolean capture);
  void removeEventListener(DOMString type, EventListener callback);
};
