callback MutationCallback = void (TextMutationRecord[] records,
                                  TextMutationObserver observer);

[Constructor(MutationCallback callback)]
interface TextMutationObserver {
  void disconnect();
  void observe(TextDocument document, TextMutationObserverInit options);
  TextMutationRecord[] takeRecords();
};
