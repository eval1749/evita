import("//tools/grit/grit_rule.gni")

grit("global_strings") {
  source = "global_strings.grd"
  resource_ids = "//evita/res/resource_ids"
  outputs = [
    "global_strings_en-US.pak",
    "grit/global_strings.js",
  ]
  grit_flags = [
    "-E",
    "target_gen_dir=" + rebase_path(target_gen_dir),
  ]
}
